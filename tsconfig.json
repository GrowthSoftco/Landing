{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@Types/*": ["./src/Types/*"],
      "@Components/*": ["./src/components/*"],
      "@Layouts/*": ["./src/layouts/*"],
      "@Assets/*": ["./src/assets/*"],
      "@Utils/*": ["./src/utils/*"],
    }
  }
}
